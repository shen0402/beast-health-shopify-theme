  <ul class="nav__items">
    {% if settings.social_instagram_link != blank %}
      <li class="nav__item">
        <a target="_blank" class="nav__link" href="{{ settings.social_instagram_link }}">
          {% comment %}<i class="icon icon-insta"></i>{% endcomment %}
          {% render 'icon' with 'social-instagram' %}
        </a>
      </li>
    {% endif %}
    {% if settings.social_twitter_link != blank %}
      <li class="nav__item">
        <a target="_blank" class="nav__link" href="{{ settings.social_twitter_link }}">
          {% comment %}<i class="icon icon-tw"></i>{% endcomment %}
          {% render 'icon' with 'social-twitter' %}
        </a>
      </li>
    {% endif %}
    {% comment %}
    {% if settings.social_youtube_link != blank %}
      <li class="nav__item">
        <a class="nav__link" href="{{ settings.social_youtube_link }}">
          <i class="icon icon-youtube"></i>
          {% render 'icon' with 'social-youtube' %}
        </a>
      </li>
    {% endif %}
    {% endcomment %}
    {% if settings.social_facebook_link != blank %}
      <li class="nav__item">
        <a class="nav__link" href="{{ settings.social_facebook_link }}">
          {% comment %}<i class="icon icon-fb"></i>{% endcomment %}
          {% render 'icon' with 'social-facebook' %}
        </a>
      </li>
    {% endif %}
  </ul>
</nav>