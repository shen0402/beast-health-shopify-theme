    <div class="feature-blog_contents">
        <div class="feature-blog_slider m-custom-controls" data-hor-slider data-dots="false" data-autoplay="false" data-custom-controls="1">
            {% for block in section.blocks %}
                {% assign bs = block.settings %}
                {% if bs.img != blank and bs.svg_logo != blank %}
                    <div class="slider_item d-flex">
                        <div class="text_wrapper">
                            {% if section.settings.subtitle != blank %}<div class="subtitle">{{ section.settings.subtitle }}</div>{% endif %}
                            <div class="item_logo">{{ bs.svg_logo }}</div>
                            {% if section.settings.comma != blank %}<div class="comma">{{ section.settings.comma }}</div>{% endif %}
                            <div class="item_text">{{ bs.text }}</div>
                        </div>
                        <div class="img_wrapper">
                            {% render 'rimage', image: bs.img, size: '800x', no_padding: true %}
                        </div>
                    </div>
                {% endif %}
            {% endfor %}
        </div>
        <div class="slider-controls">
