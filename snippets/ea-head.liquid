<link rel="preload" href="{{'custom.scss.css' | asset_url}}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript><link rel="stylesheet" href="{{'custom.scss.css' | asset_url }}"></noscript>

<script>
  document.documentElement.className = document.documentElement.className.replace('no-js', 'js');

  window.theme = {
    strings: {
      redeemoffer: {{ 'products.product.redeemoffer' | t | json }},
      addToCart: {{ 'products.product.add_to_cart' | t | json }},
      soldOut: {{ 'products.product.sold_out' | t | json }},
      preOrder: {{ 'products.product.pre_order' | t | json }},
      unavailable: {{ 'products.product.unavailable' | t | json }},
      recharge: {% if product.handle contains 'annual' %} "Subscribe" {% else %} "Subscribe For $1" {% endif %}
    },
    moneyFormat: {{ shop.money_format | json }}
  };
</script>

{% unless request.path contains '/a/review' %}
  {% if template == blank and page == blank %}
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet">
  {% endif %}
{% endunless %}

{% if template.directory == 'customers' %}
