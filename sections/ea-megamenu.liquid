{% comment %} ATTENTION!!! This is compiled version of ea-megamenu.liquid and it is overriding automatically. You should use theme development tool and sources\sections\ea-megamenu.liquid file to code and save your changes. {% endcomment %}<nav class="nav" data-accordions-container data-accordions-mobile-only="1">
  <ul class="nav__items">
    {% assign path = request.path %}

    {% for block in section.blocks %}
      {% if block.type contains 'dropdown' %}
        <li class="nav__item m-level-1 m-dropdown  {% unless block.settings.enable_desktop %}hide--desktop{% endunless %} {% unless block.settings.enable_mobile %}hide--mobile{% endunless %}" data-trigger-hover {{ block.shopify_attributes }}>
          <a onclick="dataLayer.push({'ATC': '{{ block.settings.url }}', 'type': 'link'});"  href="{{ block.settings.url }}" class="nav__link m-level-1 m-icon" data-accordion-button>
            {{ block.settings.title }}
            <i class="icon-caret-down"></i>
          </a>
          {% render 'nav-dropdown', block: block %}
        </li>

      {% elsif block.type == 'link' %}
        <li class="nav__item m-level-1 {% unless block.settings.enable_desktop %}hide--desktop{% endunless %} {% unless block.settings.enable_mobile %}hide--mobile{% endunless %}" data-megamenu="{{ block.settings.title | downcase | replace: ' ', '_' | replace: '&', 'and' }}" {{ block.shopify_attributes }}>
          <a onclick="dataLayer.push({'ATC': '{{ block.settings.url }}', 'type': 'link'});" class="nav__link m-level-1 {% if path == block.settings.url %}active{% endif %}" href="{{ block.settings.url }}">
            {{ block.settings.title }}
          </a>
        </li>      
      {% elsif block.type == 'mob_link' %}
        <li class="nav__item m-level-1 m-dropdown hide--desktop" data-trigger-hover {{ block.shopify_attributes }}>
          <a onclick="dataLayer.push({'ATC': '{{ block.settings.url }}', 'type': 'link'});" href="{{ block.settings.url }}" class="nav__link m-level-1 m-icon chat">
            {{ block.settings.icon }}
            {{ block.settings.title }}
          </a>
        </li>
      {% endif %}
    {% endfor %}
    {% comment %}
    <li class="nav__item m-level-1 m-dropdown" data-trigger-hover {{ block.shopify_attributes }}>
      <a href="{{ block.settings.url }}" class="nav__link m-level-1 m-icon chat hide--desktop" data-accordion-button>
        <svg xmlns="http://www.w3.org/2000/svg" width="21.609" height="20.898" viewBox="0 0 21.609 20.898"> <path d="M16.636 5a10.547 10.547 0 0 0-10.8 10.256 9.96 9.96 0 0 0 2.37 6.41l-1.584 3.555a.422.422 0 0 0 .078.479.544.544 0 0 0 .408.193h.076l5.835-.968a11.566 11.566 0 0 0 3.619.605 10.547 10.547 0 0 0 10.8-10.256A10.567 10.567 0 0 0 16.636 5zm0 19.545a10.17 10.17 0 0 1-3.389-.581.466.466 0 0 0-.255-.024l-5.045.846 1.35-3a.468.468 0 0 0-.076-.508A8.978 8.978 0 0 1 6.9 15.255a9.472 9.472 0 0 1 9.734-9.288 9.549 9.549 0 0 1 9.785 9.289 9.549 9.549 0 0 1-9.785 9.289z" transform="translate(-5.832 -5)"/> <path d="M31.719 46.32a1.174 1.174 0 0 1-2.344 0 1.174 1.174 0 0 1 2.344 0" transform="translate(-23.616 -35.87)"/> <path d="M47.552 46.32a1.174 1.174 0 0 1-2.345 0 1.174 1.174 0 0 1 2.345 0" transform="translate(-35.575 -35.87)"/> <path d="M63.387 46.32a1.174 1.174 0 0 1-2.344 0 1.174 1.174 0 0 1 2.344 0" transform="translate(-47.538 -35.87)"/> </svg>
        Chat with us
      </a>
      {% render 'nav-dropdown', block: block %}
    </li>
    {% endcomment %}
  </ul>
</nav> <!-- /.site-header__nav -->

